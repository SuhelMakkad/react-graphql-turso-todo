{
  "name": "react-graphql-turso-todo-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate:sqlite --out ./src/drizzle/migrations --breakpoints --schema=./src/drizzle/schema.ts",
    "db:migrate": "tsx src/drizzle/migrate",
    "gql:compile": "graphql-codegen",
    "gql:watch": "graphql-codegen -w",
    "build": "rimraf dist && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && copyfiles -f src/graphql/schema/schema.graphql dist/graphql/schema",
    "add-build": "git add dist",
    "build-start": "pnpm build && node ./dist/index.js",
    "start": "node ./dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "pre-commit": [
    "build",
    "add-build"
  ],
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@libsql/client": "0.4.0-pre.5",
    "copyfiles": "^2.4.1",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "graphql": "^16.8.1",
    "jose": "^5.1.3",
    "mysql2": "^3.6.5",
    "react-graphql-turso-todo server": "link:",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-resolvers": "^4.0.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "dotenv": "^16.3.1",
    "drizzle-kit": "^0.20.7",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@parcel/watcher": "^2.3.0"
  }
}
